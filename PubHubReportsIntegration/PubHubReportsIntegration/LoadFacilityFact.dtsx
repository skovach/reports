    /// <param name="Row">The row that is currently passing through the component</param>
    public override void MonthInput_ProcessInputRow(MonthInputBuffer Row)
    {
        if (string.IsNullOrWhiteSpace(Row.Month)) return;
        var date = Row.Month.Split('-');
        if (date.Length == 0 || date.Length < 2)
        {
            SetDefaultDate(Row);
            return;
        }
        var yearParsed = int.TryParse($"20{date[1]}", out int year);
        int month = GetMonth(date[0]);

        Row.MonthParsed = GetMonth(date[0]);
        Row.Year = yearParsed ? year : default(int);
    }

    private void SetDefaultDate(MonthInputBuffer Row)
    {
        Row.MonthParsed = 0;
        Row.Year = 0;
    }

    private int GetMonth(string month)
    {
        switch (month.ToLower())
        {
            case "jan":
                return 1;
            case "feb":
                return 2;
            case "mar":
                return 3;
            case "apr":
                return 4;
            case "may":
                return 5;
            case "jun":
                return 6;
            case "jul":
                return 7;
            case "aug":
                return 8;
            case "sep":
                return 9;
            case "oct":
                return 10;
            case "nov":
                return 11;
            case "dev":
                return 12;
            default:
                return 0;
        }
    }
